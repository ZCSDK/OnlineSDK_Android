<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/sobot_online_app_bg_color"
    android:orientation="vertical"
    android:paddingTop="12dp"
    android:paddingRight="15dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="访客来源"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_search_engines"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="访问着陆页"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_loading_page"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="对话发起页"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_launch_page"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="接入渠道"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_access_channel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="技能组"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_skill_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="排队时长"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_line_up_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="IP地址"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_ip_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="终端"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_terminal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="系统"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_user_system"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="历史访问次数"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="搜索词"
            android:textColor="@color/sobot_online_common_gray5"
            android:textSize="@dimen/sobot_sp_14" />

        <TextView
            android:id="@+id/tv_visitor_search_keywords"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:text="--"
            android:textColor="@color/sobot_online_common_gray1"
            android:textSize="@dimen/sobot_sp_14" />
    </LinearLayout>


</LinearLayout>